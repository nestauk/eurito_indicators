{
	"author": "nestauk (https://www.nesta.org.uk/)",
	"dependencies": {
		"@svizzle/atlas": "^0.9.0",
		"@svizzle/request": "^0.5.2",
		"@svizzle/time_region_value": "^0.9.0",
		"@svizzle/ui": "^0.9.0",
		"@svizzle/utils": "^0.19.0",
		"d3-array": "^3.2.1",
		"lamb": "^0.61.0",
		"node-fetch": "^3.2.10"
	},
	"description": "EURITO indicators UI",
	"devDependencies": {
		"@modyfi/vite-plugin-yaml": "^1.0.3",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-netlify": "^1.0.0",
		"@sveltejs/kit": "^1.0.1",
		"@svizzle/dev": "^0.6.1",
		"@svizzle/file": "^0.14.1",
		"bowser": "^2.11.0",
		"browserstack-capabilities": "^0.7.0",
		"browserstack-local": "^1.5.1",
		"cpy": "^9.0.1",
		"del": "^7.0.0",
		"eslint": "^8.24.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"js-yaml": "^4.1.0",
		"just-compare": "^2.2.2",
		"lighthouse": "^9.6.7",
		"mdsvex": "^0.10.6",
		"mkdirp": "^1.0.4",
		"mocha": "^10.0.0",
		"npm-run-all": "^4.1.5",
		"pa11y": "^6.2.3",
		"pa11y-reporter-html": "^2.0.0",
		"queue-promise": "^2.2.1",
		"rimraf": "^3.0.2",
		"selenium-webdriver": "^4.5.0",
		"svelte": "^3.55.0",
		"svelte-media": "^0.1.5",
		"tempy": "^3.0.0",
		"unist-util-visit": "^4.1.1",
		"vite": "^4.0.0",
		"zip-a-folder": "1.1.5"
	},
	"license": "MIT",
	"name": "eurito_indicators_ui",
	"scripts": {
		"build": "vite build",
		"chmod": "chmod u+x src/bin/*",
		"copyatlas": "cp -R ./node_modules/@svizzle/atlas/data/dist ./static/atlas",
		"copydata": "npm run chmod && node src/bin/copy_data.js ",
		"deploy": "npm run makedata && npm run makeatlas && npm run build",
		"dev": "vite dev",
		"lighthouse": "node test/lighthouse/validate.js",
		"linksvizzles": "npm link @svizzle/atlas @svizzle/dev @svizzle/dom @svizzle/file @svizzle/geo @svizzle/time_region_value @svizzle/ui @svizzle/utils && npm list",
		"lint": "eslint .",
		"makeatlas": "rimraf ./static/atlas && npm run copyatlas",
		"makedata": "npm run makeindex && npm run rmdata && npm run copydata",
		"makeindex": "npm run chmod && node src/bin/make_index.js",
		"pa11y": "node test/pa11y/validate.js",
		"preview": "vite preview",
		"redev": "npm run rmkv && npm run dev",
		"reinstall": "npm run uninstall && npm install",
		"reportToCSV": "npm run chmod && node src/bin/reportToCSV.js",
		"rmdata": "rimraf static/data",
		"rmkit": "rimraf .svelte-kit",
		"rmkv": "npm run rmkit && npm run rmvite",
		"rmvite": "rimraf node_modules/.vite",
		"selenium": "node test/browserstack/selenium-runner.mjs",
		"uninstall": "rimraf node_modules package-lock.json",
		"unittest": "mocha --spec src/**/*.spec.js"
	},
	"type": "module"
}
